{
  "name": "javalint",
  "version": "1.0.0",
  "description": "A github app that analyses java sources",
  "author": "Lesley Lai <lesleylai@lesleylai.info>",
  "license": "ISC",
  "repository": "https://github.com/LesleyLai/javalint-bot",
  "homepage": "https://github.com/javalint-bot",
  "bugs": "https://github.com/javalint-bot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --exec \"npm start\"",
    "start": "yarn build && probot run ./build/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@types/async": "^2.4.1",
    "@types/nock": "^9.3.0",
    "async": "^2.6.2",
    "nock": "^10.0.0",
    "pmd-bin": "^1.2.0",
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^11.10.4",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.1",
    "typescript-eslint-parser": "^22.0.0"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
